<launch>
   <arg name="model" />
   <arg name="gui" default="False" />
   <param name="robot_description" textfile="$(find gripperv4)/urdf/gripperv4.urdf" />
   <param name="use_gui" value="$(arg gui)" />
   <node name="joint_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
   <node name="rviz" pkg="rviz" type="rviz" args="-d $(find gripperv4)/urdf/gripperv4.urdf" />
</launch>
